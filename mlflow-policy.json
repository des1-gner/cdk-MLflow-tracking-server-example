{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"sagemaker-mlflow:*",
				"sagemaker:DescribeMlflowTrackingServer",
				"sagemaker:ListMlflowTrackingServers",
				"sagemaker:CreatePresignedMlflowTrackingServerUrl"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject",
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::mlflow-artifacts-<your-aws-account-id>",
				"arn:aws:s3:::mlflow-artifacts-<your-aws-account-id>/*"
			]
		}
	]
}
